* {
    margin:0;
    padding:0;
}

html {
	min-height:100%;
	height:100%;
}

body {
	/*background: url(images/bg_top.png) repeat-x;*/
	position: relative;
	font: smaller Geneva, sans-serif;
    min-height:100%;
    height:100%;
}

div {
/* 	overflow:hidden; */
    height:auto;
}

h1 {
    font-family:"Lithos";
    font-size:1.8em;
    font-weight:bolder;
    color:#333;
}

h1 a {
    text-decoration:none;
    color:#333;
}

h2, h2 a{
    font-size:1.2em;
    text-align: center;
    padding: 10px;
}

h3, h3 a{
    font-size:1em;
}

ul{
    list-style:none;
}

p{
    text-align:justify;
    margin-bottom: .5em;
}

p a{
    color: #1D62B3;
}

a {
    text-decoration: none;
    color: #1D62B3;
    font-size: 1em;
}

a:hover {
    background: none;
    color: #E43C15;
}

img {
    border:none;
    background: none;
    padding: 0;
}
/* navigation */

#navigatemenu {
    float:left;
    position: relative;
    z-index:100;
    background:none;
    width: auto;
    font-size:small;
    border-radius: 0.5em;
    -moz-border-radius: 0.5em;
    -webkit-border-radius: 0.5em;
}

#navigatemenu .menuselected {
	background:#63AE24;
    color:#fff;
    font-weight:bold;
}

#nav {
	position:static;
	z-index:100;
    padding:0;
    margin:0;
    height:25px;
}

#nav ul{
    margin:0;
    padding:0;
    list-style:none;
}

#nav li {
    float:left;
    list-style:none;
}

#nav li a {
    padding:6px 8px 6px 8px;
    margin:0;
    font-weight:bold;
    text-decoration:none;
    display:block;
}

#nav li a.active {
    background:#eee;
    color:#444;
    text-decoration:none;
}

#nav ul li{
    display: block;
    position: relative;
    float: left;
    margin:0;
    padding:0;
}

#nav li ul{
    display: none;
}

#nav ul li a{
    display: block;
    text-decoration: none;
    color: #fff;
    border-top: 1px solid #333333;
    white-space: nowrap;
    text-decoration: none;
    float: left; /*\*/ float:none;
    padding:0;
    width: 150px;
    position:static;
	z-index:100;
}

#nav ul li a:hover{
    background:#0a1825;
    color:#fff;
    font-weight:bold;
}

#nav li:hover ul{
    display: block;
    position: absolute;
    font-weight:bold;
    z-index: 100;
}

#nav li:hover li{
    float: none;
}

#nav ul li a span{
    font-weight: normal;
    color: #67707a;
}

#nav li:hover a{
    color:#FFF;
    background:#63AE24; /*#555;*/
}

#nav li:hover li a:hover{
    background:#94DA3A; /*#999;*/
    color:#63AE24;
}

/* navigation ends here */

/* tab menu starts here */

#tabmenus {
    float: left;
    position: relative;
	min-width: 100px;
	margin-top:30px;
	position:static;
	display: block;
	text-decoration: none;
	border-radius: 0.5em;
    -moz-border-radius: 0.5em;
    -webkit-border-radius: 0.5em;
    
}

#tabmenus .ntabs {
	background: none;
	padding:5px;
    margin:0;
    height:18px;
    background: #63AE24;
}

.ntab {
	font-weight: bold;
	height: 25px;
    text-decoration: none;
    white-space: nowrap;
	padding:6px 8px 6px 8px;
}

.ntab:hover {
    background: #63AE24;
	text-decoration: none;
	color: #fff;
}

.ntab_a {
    color:#fff;
    font-weight:bold;
    text-decoration: none;
    padding:6px 8px 6px 8px;
}

#tabmenus .ntab_a {
	color: #fff;
}

.ntab_a:hover {
	text-decoration: none;
	color: #fff;
}


/* tab menu ends here */


